# Server configuration
PORT=<YOUR_PORT>
NODE_ENV=<development|production>

# Database configuration
DB_HOST=<YOUR_DB_HOST>
DB_USER=<YOUR_DB_USERNAME>
DB_PASSWORD=<YOUR_DB_PASSWORD>
DB_NAME=<YOUR_DB_NAME>
DB_PORT=<YOUR_DB_PORT>

# Socket.io configuration
SOCKET_CORS_ORIGIN=<YOUR_CORS_SETTING>
SOCKET_PATH=<YOUR_SOCKET_PATH>

# Notification polling interval (in milliseconds)
NOTIFICATION_POLL_INTERVAL=<YOUR_POLL_INTERVAL>

# ci4 backend - CRUD
API_BASE_URL=<YOUR_API_URL>

# Example configurations:
# For local development: API_BASE_URL=http://localhost:<port>
# For Docker: API_BASE_URL=http://host.docker.internal:<port>